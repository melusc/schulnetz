{
	"name": "schulnetz-cli",
	"packageManager": "yarn@3.5.0",
	"private": true,
	"type": "module",
	"devDependencies": {
		"@lusc/tsconfig": "^2.2.0",
		"esbuild": "^0.17.12",
		"prettier": "^2.8.6",
		"typescript": "^5.0.2",
		"xo": "^0.53.1"
	},
	"scripts": {
		"build": "tsc && yarn esbuild cli.ts --bundle --packages=external --outfile=dist/cli.js --format=esm --sourcemap",
		"fmt": "prettier -w . && xo --fix",
		"start": "node dist/cli"
	},
	"workspaces": [
		"workspaces/*"
	],
	"dependencies": {
		"commander": "^10.0.0"
	}
}

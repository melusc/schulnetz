{
	"name": "schulnetz-cli",
	"packageManager": "yarn@3.6.0",
	"private": true,
	"type": "module",
	"devDependencies": {
		"@lusc/tsconfig": "^2.3.0",
		"esbuild": "^0.17.19",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3",
		"xo": "^0.54.2"
	},
	"scripts": {
		"build": "tsc && yarn esbuild cli.ts --bundle --packages=external --outfile=dist/cli.js --format=esm --sourcemap",
		"fmt": "prettier -w . && xo --fix",
		"start": "node dist/cli"
	},
	"workspaces": [
		"workspaces/*"
	],
	"dependencies": {
		"commander": "^10.0.1"
	}
}
